[server1]
server1 ansible_host=192.168.2.191 

[server2]
server2 ansible_host=192.168.2.192 ansible_python_interpreter=/usr/bin/python3

[servers]
server1 ansible_host=192.168.2.191 ansible_python_interpreter=/usr/bin/python3
server2 ansible_host=192.168.2.192 ansible_python_interpreter=/usr/bin/python3


[webservers]
webserver1 ansible_host=192.168.2.191 ansible_python_interpreter=/usr/bin/python3
webserver2 ansible_host=192.168.2.192 ansible_python_interpreter=/usr/bin/python3

